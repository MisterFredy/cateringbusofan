<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tia.fredy">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.hardware.camera" />
    <uses-permission android:name="android.permission.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity" />
        <activity
            android:name=".rekapmanual"
            android:parentActivityName=".Main2Activity" />
        <activity
            android:name=".RkpAbsn"
            android:parentActivityName=".Main2Activity" />
        <activity
            android:name=".qrcode"
            android:parentActivityName=".Main2Activity" />
        <activity android:name=".scan"></activity>
    </application>

</manifest>