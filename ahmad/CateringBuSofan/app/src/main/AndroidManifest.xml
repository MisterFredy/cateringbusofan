            android:theme="@style/AppTheme" />
        <activity
            android:name=".PackageBuffetAActivity"
            android:parentActivityName=".CatalogueActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".PackagePondokanActivity"
