<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.akhmadfaizin.cateringbusofan">

    <!-- Internet Permission -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UbahProfileActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".GantiPasswordActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".CatalogueActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".PesananActivity"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".DetailMenuCardActivity" />
        <activity android:name=".DetailItemActivity" />
        <activity
            android:name=".PackageSnackBActivity"
            android:parentActivityName=".CatalogueActivity" />
        <activity
            android:name=".PackageSnackAActivity"
            android:parentActivityName=".CatalogueActivity" />
        <activity
            android:name=".PackageBuffetCActivity"
            android:parentActivityName=".CatalogueActivity" />
        <activity android:name=".PackageDeliveryDetailActivity"></activity>
    </application>

</manifest>